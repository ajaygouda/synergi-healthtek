{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "userId"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "customer"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "customer"
    }
  }
}
